{
  "name": "reselect-change-memoize",
  "version": "1.0.2",
  "description": "A memoize function for reselect which will make a callback when a result changes",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/reselect-change-memoize.js && NODE_ENV=production webpack src/index.js dist/reselect-change-memoize.min.js",
    "lint": "eslint src test",
    "test": "NODE_ENV=test mocha",
    "test:watch": "NODE_ENV=test mocha --watch",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kbrownlees/reselect-change-memoize.git"
  },
  "keywords": [
    "reselect",
    "memoize",
    "logging"
  ],
  "author": "Kieran Brownlees",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kbrownlees/reselect-change-memoize/issues"
  },
  "homepage": "https://github.com/kbrownlees/reselect-change-memoize",
  "dependencies": {
    "reselect": ">=1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "eslint": "^2.7",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "expect": "^1.16.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
